{
  "name": "async-iterators",
  "version": "0.0.1",
  "description": "Iterator functions for async functions (or Promise)",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "main": "lib/asyncIterators.js",
  "devDependencies": {
    "@types/jest": "~22.1.1",
    "@types/node": "~8.5.10",
    "barrelsby": "0.0.8",
    "husky": "^0.14.3",
    "jest": "~22.1.4",
    "lint-staged": "^7.0.0",
    "prettier": "1.10.2",
    "rimraf": "~2.6.2",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "tsutils": "~2.21.0",
    "typedoc": "^0.10.0",
    "typescript": "~2.7.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run doc",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "generate-barrels": "barrelsby --delete -d src/",
    "doc": "npx typedoc src/ --out doc/"
  },
  "author": "Ale Figueroa  <me@ale.world>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yknx4/async-iterators.git"
  },
  "bugs": {
    "url": "https://github.com/yknx4/async-iterators/issues"
  },
  "homepage": "https://github.com/yknx4/async-iterators#readme",
  "dependencies": {
    "tslib": "~1.9.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": ["prettier --write", "npm run lint --fix", "git add"],
    "src/**/*.{json,md}": ["prettier --write", "git add"],
    "**/*.{json,md}": ["prettier --write", "git add"]
  }
}
